<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alazharbsd.masjid.masjidapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.LOCATION_HARDWARE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/mosquedomes"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mosquedomes"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".KegiatanActivity" />
        <activity android:name=".KegiatanDetailActivity" />
        <activity android:name=".KhutbahActivity" />
        <activity android:name=".KhutbahdetailActivity" />
        <activity android:name=".ImamActivity" />
        <activity android:name=".ImamDetailActivity" />
        <activity android:name=".RamadhanActivity" />
        <activity android:name=".RamadhanDetailActivity" />
        <activity android:name=".PengumumanDetailActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".VideoDetailActivity" />
        <activity android:name=".ArtikelActivity" />
        <activity android:name=".ArtikelDetailActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".KegiatanDetailDetailActivity" />
        <activity android:name=".KataDetailActivity" />
        <activity android:name=".KiblatActivity" />
        <activity android:name=".UstadzActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ArtikelkategoriActivity"></activity>
        <receiver android:name=".Alarmreceiver" android:enabled="true" />
        <service android:name=".Alarmservice" android:enabled="true" />
    </application>

</manifest>